<%# Newsletter %>
<section class="py-16 relative overflow-hidden" data-theme-target="container">
  <div class="absolute inset-0 -z-10" data-class-light="bg-blue-50" data-class-dark="bg-gray-900"></div>
  
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="max-w-xl mx-auto text-center">
      <h2 class="text-3xl font-bold tracking-tight sm:text-4xl"
          data-class-light="text-gray-900" 
          data-class-dark="text-white">
        Stay updated with Supper Ghana
      </h2>
      <p class="mt-4 text-lg"
         data-class-light="text-gray-600" 
         data-class-dark="text-gray-300">
        Subscribe to our newsletter for the latest features, tips, and special offers.
      </p>
      
      <div class="mt-8" data-controller="newsletter">
        <form class="sm:flex justify-center" data-action="newsletter#submit">
          <label for="email-address" class="sr-only">Email address</label>
          <input id="email-address" name="email" type="email" autocomplete="email" required 
                 class="w-full px-5 py-3 focus:ring-2 focus:outline-none sm:max-w-md rounded-l-md"
                 data-class-light="placeholder:text-gray-500 text-gray-900 border-gray-300 focus:border-blue-500 focus:ring-blue-500"
                 data-class-dark="placeholder:text-gray-400 text-white bg-gray-800 border-gray-700 focus:border-blue-400 focus:ring-blue-400"
                 placeholder="Enter your email"
                 data-newsletter-target="email">
          <div class="mt-3 rounded-md sm:mt-0 sm:flex-shrink-0">
            <button type="submit" 
                    class="w-full flex items-center justify-center px-5 py-3 text-base font-medium rounded-r-md focus:outline-none focus:ring-2 focus:ring-offset-2 transition-colors"
                    data-class-light="bg-blue-600 hover:bg-blue-700 text-white focus:ring-blue-500"
                    data-class-dark="bg-blue-500 hover:bg-blue-400 text-gray-900 focus:ring-blue-400">
              Subscribe
            </button>
          </div>
        </form>
        
        <!-- Success message (hidden by default) -->
